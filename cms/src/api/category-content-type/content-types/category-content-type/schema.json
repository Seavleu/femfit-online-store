{
  "kind": "collectionType",
  "collectionName": "category_content_types",
  "info": {
    "singularName": "category-content-type",
    "pluralName": "category-content-types",
    "displayName": "Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "sortOrder": {
      "type": "integer"
    },
    "seo": {
      "type": "component",
      "component": "seo-settings.seo-settings",
      "repeatable": true
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category-content-type.category-content-type",
      "mappedBy": "parent"
    },
    "parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category-content-type.category-content-type",
      "inversedBy": "categories"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-content-type.product-content-type",
      "mappedBy": "category"
    }
  }
}
